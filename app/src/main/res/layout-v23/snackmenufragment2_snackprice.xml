<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@color/BackgroundTwo"
    android:elevation="10dp">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/Istpricebutton"
        android:textSize="20dp"
        android:textColor="@color/TextColorTwo_Snack_A"
        android:layout_marginTop="39dp"
        android:hint="@string/ButtonHintSelect_snackPrice_snackActivity"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:allowUndo="false"
        android:clickable="false"
        android:background="@drawable/sa_snackprice_buttons_circleside"
        android:contextClickable="false"
        android:layout_alignParentTop="true"
        android:fontFamily="14pt"
        android:layout_alignParentStart="true"
        android:layout_marginEnd="40dp"
        android:layout_marginStart="10dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/Secondpricebutton"
        android:textSize="20dp"
        android:textColor="@color/TextColorTwo_Snack_A"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:background="@drawable/sa_snackprice_buttons_circleside"
        android:layout_alignTop="@+id/Istpricebutton"
        android:layout_alignParentEnd="true"
        android:fontFamily="14pt"
        android:hint="@string/ButtonHintSelectv23_snackPrice_snackActivity"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="40dp"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/Thirdpricebutton"
        android:background="@drawable/sa_snackprice_buttons_circleside"
        android:layout_marginTop="82dp"
        android:fontFamily="14pt"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:textSize="20dp"
        android:textColor="@color/TextColorTwo_Snack_A"
        android:hint="@string/ButtonHintSelectv23_snackPrice_snackActivity2"
        android:layout_below="@+id/Istpricebutton"
        android:layout_alignParentStart="true"
        android:layout_marginEnd="40dp"
        android:layout_marginStart="10dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/Fourthpricebutton"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:textSize="20dp"
        android:textColor="@color/TextColorTwo_Snack_A"
        android:background="@drawable/sa_snackprice_buttons_circleside"
        android:fontFamily="14pt"
        android:hint="@string/ButtonHintSelectv23_snackPrice_snackActivity_3"
        android:layout_above="@+id/Fifthpricebutton"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/Thirdpricebutton"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="40dp"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/Fifthpricebutton"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:textSize="20dp"
        android:textColor="@color/TextColorTwo_Snack_A"
        android:background="@drawable/sa_snackprice_buttons_circleside"
        android:layout_marginTop="91dp"
        android:fontFamily="14pt"
        android:hint="@string/ButtonHintSelectv23_snackPrice_snackActivity_4"
        android:layout_below="@+id/Thirdpricebutton"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="40dp"
        android:layout_marginStart="10dp"/>


